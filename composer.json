{
  "name": "betalabs/engine-phpsdk",
  "description": "SDK for Engine",
  "authors": [],
  "require": {
    "php": "^8.0",
    "php-di/php-di": "^5.4|^6.0",
    "nesbot/carbon": "^2.26",
    "aura/router": "^3.1",
    "symfony/cache": "^5.1",
    "symfony/http-kernel": "^5.1",
    "symfony/mime": "^5.1",
    "symfony/http-foundation": "^5.1",
    "symfony/var-dumper": "^5.1",
    "symfony/var-exporter": "^5.1",
    "predis/predis": "^1.1",
    "phpdocumentor/reflection-docblock": "^5.1",
    "phpdocumentor/type-resolver": "^1.4.0",
    "betalabs/guzzle6-adapter": "^4.0",
    "laminas/laminas-diactoros": "^2.25"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Betalabs\\Engine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Betalabs\\Engine\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
